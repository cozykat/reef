				<common:configFile fileName="onelines/residential.xml" mimeType="oneline/xml"/>
			</common:info>
			<equip:equipment name="Residential"/>
		</equip:equipment>

		<equip:equipment name="Sites" addParentNames="false">
			<equip:type name="Root"/>
