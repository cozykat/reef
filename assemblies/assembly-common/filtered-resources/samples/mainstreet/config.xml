				<common:configFile fileName="onelines/mainst.xml" mimeType="oneline/xml"/>
			</common:info>
			<equip:equipment name="MainSt"/>
		</equip:equipment>

		<equip:equipment name="ResidentialSite" addParentNames="false">
			<equip:type name="Site"/>
