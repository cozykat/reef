<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<configuration version="1.0"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns="configuration.loader.reef.totalgrid.org"
               xmlns:eq="equipment.loader.reef.totalgrid.org"
               xmlns:cm="communications.loader.reef.totalgrid.org"
               xmlns:common="common.loader.reef.totalgrid.org">
  <messageModel>
    <messageSet name="System" severity="8" type="LOG">
      <message name="UserLogin" type="EVENT">User login {status} {reason}</message>
      <message name="UserLogout" type="EVENT">User logged out</message>
      <message name="SubsystemStarting">Subsystem is starting</message>
      <message name="SubsystemStarted">Subsystem has started</message>
      <message name="SubsystemStopping">Subsystem is stopping</message>
      <message name="SubsystemStopped">Subsystem has stopped</message>
    </messageSet>
    <messageSet name="Scada" severity="6" type="ALARM" state="UNACK_SILENT">
      <message name="ControlExe">User executed control {control} on device {device}</message>
      <message name="OutOfNominal" severity="4">Measurement not in nominal range: {value}{unit} validity {validity}</message>
      <message name="OutOfReasonable" severity="3" state="UNACK_AUDIBLE">Measurement not reasonable: {value}{unit} validity {validity}</message>
    </messageSet>
  </messageModel>
  <actionModel>
    <actionSet name="RLC" priority="100">
      <rising>
        <message name="Scada.OutOfReasonable" />
      </rising>
      <high moreActions="false">
        <stripValue />
        <setAbnormal />
      </high>
    </actionSet>
    <actionSet name="Nominal" priority="500">
      <rising>
        <message name="Scada.OutOfNominal" />
      </rising>
    </actionSet>
    <actionSet name="Everything" priority="1000">
      <rising>
        <message name="Scada.OutOfNominal" />
      </rising>
      <high>
        <!-- <quality validity="questionable" source="PROCESS" detailQuality="2" test="false" operatorBlocked="false"/> -->
        <stripValue />
        <setBool value="true" />
        <setUnit unit="var" />
      </high>
      <transition></transition>
    </actionSet>
    <!--/actionModel>

    <actionModel>

        <actionSet name="Nominal">
            <rising>
                <message name="Scada.OutOfNominal"/>
            </rising>
            <high>
                <setAbnormal/>
            </high>
        </actionSet-->
    <actionSet name="Abnormal">
      <high>
        <setAbnormal />
      </high>
    </actionSet>
  </actionModel>
  <eq:equipmentModel>
    <eq:profiles>
      <!-- Status Point Profiles-->
      <eq:pointProfile name="ChargeDis_NoAlm">
        <eq:unexpected stringValue="Discharge" actionSet="Abnormal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="Discharge" />
          <eq:valueMap fromValue="false" toValue="Charge" />
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="FID_BKR">
        <eq:unexpected stringValue="Open" actionSet="Nominal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="Closed" />
          <eq:valueMap fromValue="false" toValue="Open" />
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="NormallyTrueAlarmed">
        <eq:unexpected stringValue="Alarm" actionSet="Nominal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="Alarm" />
          <eq:valueMap fromValue="false" toValue="Normal" />
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="NormallyFalseAlarmed">
        <eq:unexpected stringValue="Alarm" actionSet="Nominal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="Normal" />
          <eq:valueMap fromValue="false" toValue="Alarm" />
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="AutoMan_NoAlm">
        <eq:unexpected stringValue="Manual" actionSet="Abnormal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="Manual" />
          <eq:valueMap fromValue="false" toValue="Auto"></eq:valueMap>
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="OnOff_NoAlm">
        <eq:unexpected stringValue="Off" actionSet="Abnormal" />
        <eq:transform fromUnit="raw" toUnit="status" transformationType="status">
          <eq:valueMap fromValue="true" toValue="On" />
          <eq:valueMap fromValue="false" toValue="Off" />
        </eq:transform>
      </eq:pointProfile>
      <eq:pointProfile name="GenMWh">
        <eq:range actionSet="RLC" low="0" high="12000" deadband="1" />
      </eq:pointProfile>
      <!-- Equipment Profiles-->
      <eq:equipmentProfile name="MicrogridProfile">
        <eq:type name="Microgrid" />
      </eq:equipmentProfile>
      <eq:equipmentProfile name="FullBreaker">
        <eq:type name="Breaker" />
        <eq:control name="Open" />
        <eq:control name="Close" />
        <eq:status name="Status" unit="status" pointProfile="FID_BKR">
          <eq:control name="Open" />
          <eq:control name="Close" />
        </eq:status>
        <eq:status name="Overcurrent" unit="status" pointProfile="NormallyTrueAlarmed" />
      </eq:equipmentProfile>
      <eq:equipmentProfile name="KvKwkVar_Meas">
        <eq:type name="PV" />
        <eq:analog name="kV" unit="kV"></eq:analog>
        <eq:analog name="kW" unit="kW"></eq:analog>
        <eq:analog name="kVar" unit="kVar"></eq:analog>
      </eq:equipmentProfile>
      <eq:equipmentProfile name="DES">
        <eq:type name="PV" />
        <eq:analog name="kV" unit="kV"></eq:analog>
        <eq:analog name="kW" unit="kW">
          <eq:setpoint name="Pset" />
        </eq:analog>
        <eq:analog name="kVar" unit="kVar">
          <eq:setpoint name="Qset" />
        </eq:analog>
        <eq:analog name="%SOC" unit="%SOC"></eq:analog>
        <eq:setpoint name="Pset" />
        <eq:setpoint name="Qset" />
        <eq:control name="Charge" />
        <eq:control name="Discharge" />
        <eq:status name="Mode" unit="status" pointProfile="ChargeDis_NoAlm">
          <eq:control name="Charge" />
          <eq:control name="Discharge" />
        </eq:status>
        <eq:analog name="Amps" unit="Amps"></eq:analog>
      </eq:equipmentProfile>
      <eq:equipmentProfile name="Line_Meas">
        <eq:type name="Line" />
        <eq:analog name="Amps" unit="Amps"></eq:analog>
        <eq:analog name="kW" unit="kW"></eq:analog>
      </eq:equipmentProfile>
      <eq:equipmentProfile name="SST">
        <eq:type name="SST" />
        <eq:analog name="kW_a" unit="kW"></eq:analog>
        <eq:setpoint name="Qref_a" />
        <eq:setpoint name="Qref_b" />
        <eq:setpoint name="Qref_c" />
        <eq:analog name="kVar_a" unit="kVar">
          <eq:setpoint name="Qref_a" />
        </eq:analog>
        <eq:analog name="kVar_b" unit="kVar">
          <eq:setpoint name="Qref_b" />
        </eq:analog>
        <eq:analog name="kVar_c" unit="kVar">
          <eq:setpoint name="Qref_c" />
        </eq:analog>
        <eq:analog name="kW_b" unit="kW"></eq:analog>
        <eq:analog name="kW_c" unit="kW"></eq:analog>
        <eq:analog name="kV_a" unit="kV"></eq:analog>
        <eq:analog name="kV_b" unit="kV"></eq:analog>
        <eq:analog name="kV_c" unit="kV"></eq:analog>
        <eq:status name="Qsst_a" unit="status" pointProfile="OnOff_NoAlm" />
        <eq:status name="Qsst_b" unit="status" pointProfile="OnOff_NoAlm" />
        <eq:status name="Qsst_c" unit="status" pointProfile="OnOff_NoAlm" />
      </eq:equipmentProfile>
      <eq:equipmentProfile name="Bus_Meas">
        <eq:type name="Bus" />
        <eq:analog name="kW_a" unit="kW"></eq:analog>
        <eq:analog name="kVar_a" unit="kVar"></eq:analog>
        <eq:analog name="kVar_b" unit="kVar"></eq:analog>
        <eq:analog name="kVar_c" unit="kVar"></eq:analog>
        <eq:analog name="kW_b" unit="kW"></eq:analog>
        <eq:analog name="kW_c" unit="kW"></eq:analog>
        <eq:analog name="kV_a" unit="kV"></eq:analog>
        <eq:analog name="kV_b" unit="kV"></eq:analog>
        <eq:analog name="kV_c" unit="kV"></eq:analog>
      </eq:equipmentProfile>
    </eq:profiles>
    <!-- Equipment Definition-->
    <eq:equipment name="MainSt">
      <!--Start MainSt Equipment Configuration-->
      <eq:equipmentProfile name="MicrogridProfile" />
      <eq:equipment name="FID1">
        <eq:equipmentProfile name="FullBreaker" />
      </eq:equipment>
      <eq:equipment name="FID2">
        <eq:equipmentProfile name="FullBreaker" />
      </eq:equipment>
      <eq:equipment name="FID3">
        <eq:equipmentProfile name="FullBreaker" />
      </eq:equipment>
      <eq:equipment name="FID4">
        <eq:equipmentProfile name="FullBreaker" />
      </eq:equipment>
      <!--<eq:equipment name="CB1" xmlns:eq="equipment.loader.reef.totalgrid.org"><eq:equipmentProfile name="FullBreaker" /></eq:equipment>-->
      <eq:equipment name="Substation1">
        <eq:equipmentProfile name="Bus_Meas" />
        <eq:analog name="Frequency" unit="Hz"></eq:analog>
      </eq:equipment>
      <eq:equipment name="Bus1">
        <eq:type name="Bus" />
        <eq:equipmentProfile name="Bus_Meas" />
      </eq:equipment>
      <eq:equipment name="Bus2">
        <eq:type name="Bus" />
