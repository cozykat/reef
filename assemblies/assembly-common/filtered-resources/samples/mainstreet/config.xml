			</equip:equipment>

			<equip:equipment name="ChargePointStation">
				<equip:equipmentProfile name="ChargePointUsage"/>
			</equip:equipment>

		</equip:equipment>

		<equip:equipment name="MicroGrids" addParentNames="false">
			<equip:type name="Root"/>
