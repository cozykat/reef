<settings>
    <!-- /home/teamcity/.BuildServer/config/reef-metrics/plugin-settings.xml -->
    <!-- /home/teamcity/.BuildServer/config/reef/plugin-settings.xml replacing bt45 with bt43 -->
    <custom-graphs>
        <graph title="Number of Objects in the system">
            <valueType key="systemStatsallPointsvalueaverage" title="Points" buildTypeId="bt45"/>
            <valueType key="systemStatsallCommandsvalueaverage" title="Commands" buildTypeId="bt45"/>
            <valueType key="systemStatsallEntitiesvalueaverage" title="Entities" buildTypeId="bt45"/>
            <valueType key="systemStatsallEndpointsvalueaverage" title="Endpoints" buildTypeId="bt45"/>
            <valueType key="systemStatsallApplicationsvalueaverage" title="Applications" buildTypeId="bt45"/>
        </graph>
        <graph title="Get allXxxxx request times">
            <valueType key="systemRequestsrequesttimemin" title="min" buildTypeId="bt45"/>
            <valueType key="systemRequestsrequesttimemax" title="max" buildTypeId="bt45"/>
            <valueType key="systemRequestsrequesttimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Measurements per Point">
            <valueType key="measStatstotalMeasvaluemin" title="min" buildTypeId="bt45"/>
            <valueType key="measStatstotalMeasvaluemax" title="max" buildTypeId="bt45"/>
            <valueType key="measStatstotalMeasvalueaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Oldest measurement per Point (seconds)">
            <valueType key="measStatsoldestMeasvaluemin" title="min" buildTypeId="bt45"/>
            <valueType key="measStatsoldestMeasvaluemax" title="max" buildTypeId="bt45"/>
            <valueType key="measStatsoldestMeasvalueaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Retrieving oldest 10 measurements">
            <valueType key="measHistoryoldest10Meastimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistoryoldest10Meastimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistoryoldest10Meastimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Retrieving oldest 1000 measurements">
            <valueType key="measHistoryoldest1000Meastimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistoryoldest1000Meastimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistoryoldest1000Meastimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Retrieving newest 10 measurements">
            <valueType key="measHistorynewest10Meastimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistorynewest10Meastimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistorynewest10Meastimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Retrieving newest 1000 measurements">
            <valueType key="measHistorynewest1000Meastimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistorynewest1000Meastimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistorynewest1000Meastimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Retrieving all measurements (up to 10000 limit)">
            <valueType key="measHistoryallMeastimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistoryallMeastimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistoryallMeastimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Getting measurement statistics">
            <valueType key="measHistorymeasStattimemin" title="min" buildTypeId="bt45"/>
            <valueType key="measHistorymeasStattimemax" title="max" buildTypeId="bt45"/>
            <valueType key="measHistorymeasStattimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Benchmark Endpoint disable to COMMS_DOWN Time (milliseconds)">
            <valueType key="endpointbenchmark-COMMSDOWNStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
            <valueType key="endpointbenchmark-COMMSDOWNStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
            <valueType key="endpointbenchmark-COMMSDOWNStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Benchmark Endpoint enable to COMMS_UP Time (milliseconds)">
            <valueType key="endpointbenchmark-COMMSUPStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
            <valueType key="endpointbenchmark-COMMSUPStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
            <valueType key="endpointbenchmark-COMMSUPStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Overall Time to add/remove Endpoints in parallel (5 writers)">
            <valueType key="endpointLoadingoveralladdEndpoint-10-20-5-50timemin" title="Add 10 Endpoints, 200 Points" buildTypeId="bt45"/>
            <valueType key="endpointLoadingoveralldeleteEndpoint-10-20-5-50timemin" title="Delete 10 Endpoints, 300 Points"
                       buildTypeId="bt45"/>
        </graph>
        <graph title="Time to add endpoints with 30 points each (5 writers)">
            <valueType key="endpointLoadingaddEndpoint-10-20-5-50timemin" title="min" buildTypeId="bt45"/>
            <valueType key="endpointLoadingaddEndpoint-10-20-5-50timemax" title="max" buildTypeId="bt45"/>
            <valueType key="endpointLoadingaddEndpoint-10-20-5-50timeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Time to delete endpoints with 30 points and 15000 meas total (5 writers)">
            <valueType key="endpointLoadingdeleteEndpoint-10-20-5-50timemin" title="min" buildTypeId="bt45"/>
            <valueType key="endpointLoadingdeleteEndpoint-10-20-5-50timemax" title="max" buildTypeId="bt45"/>
            <valueType key="endpointLoadingdeleteEndpoint-10-20-5-50timeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Overall Time to publish 5000 measurements in batches of 25">
            <valueType key="concurrentPublishOverall1-25-5000timemin" title="1 writers" buildTypeId="bt45"/>
            <valueType key="concurrentPublishOverall5-25-5000timemin" title="5 writers" buildTypeId="bt45"/>
            <valueType key="concurrentPublishOverall10-25-5000timemin" title="10 writers" buildTypeId="bt45"/>
        </graph>
        <graph title="Time to publish a 25 measurement batch (1 writers)">
            <valueType key="concurrentPublishing1-25-5000timemin" title="min" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing1-25-5000timemax" title="max" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing1-25-5000timeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Time to publish a 25 measurement batch (5 writers)">
            <valueType key="concurrentPublishing5-25-5000timemin" title="min" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing5-25-5000timemax" title="max" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing5-25-5000timeaverage" title="average" buildTypeId="bt45"/>
        </graph>
        <graph title="Time to publish a 25 measurement batch (10 writers)">
            <valueType key="concurrentPublishing10-25-5000timemin" title="min" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing10-25-5000timemax" title="max" buildTypeId="bt45"/>
            <valueType key="concurrentPublishing10-25-5000timeaverage" title="average" buildTypeId="bt45"/>
        </graph>
    </custom-graphs>
</settings>