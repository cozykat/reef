<settings>
    <!-- /home/teamcity/.BuildServer/config/reef-metrics/plugin-settings.xml -->
    <!-- /home/teamcity/.BuildServer/config/reef/plugin-settings.xml replacing bt45 with bt43 -->
	<custom-graphs>
        <graph title="Number of Objects in the system">
			<valueType key="allPointsvalueaverage" title="Points" buildTypeId="bt45"/>
			<valueType key="allCommandsvalueaverage" title="Commands" buildTypeId="bt45"/>
			<valueType key="allEntitiesvalueaverage" title="Entities" buildTypeId="bt45"/>
            <valueType key="allEndpointsvalueaverage" title="Endpoints" buildTypeId="bt45"/>
            <valueType key="allApplicationsvalueaverage" title="Applications" buildTypeId="bt45"/>
		</graph>
        <graph title="Get allXxxxx request times">
			<valueType key="requesttimemin" title="min" buildTypeId="bt45"/>
			<valueType key="requesttimemax" title="max" buildTypeId="bt45"/>
			<valueType key="requesttimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Measurements per Point">
			<valueType key="totalMeasvaluemin" title="min" buildTypeId="bt45"/>
			<valueType key="totalMeasvaluemax" title="max" buildTypeId="bt45"/>
			<valueType key="totalMeasvalueaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Oldest measurement per Point (seconds)">
			<valueType key="oldestMeasvaluemin" title="min" buildTypeId="bt45"/>
			<valueType key="oldestMeasvaluemax" title="max" buildTypeId="bt45"/>
			<valueType key="oldestMeasvalueaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Retrieving oldest 10 measurements">
			<valueType key="oldest10Meastimemin" title="min" buildTypeId="bt45"/>
			<valueType key="oldest10Meastimemax" title="max" buildTypeId="bt45"/>
			<valueType key="oldest10Meastimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Retrieving oldest 1000 measurements">
			<valueType key="oldest1000Meastimemin" title="min" buildTypeId="bt45"/>
			<valueType key="oldest1000Meastimemax" title="max" buildTypeId="bt45"/>
			<valueType key="oldest1000Meastimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Retrieving newest 10 measurements">
			<valueType key="newest10Meastimemin" title="min" buildTypeId="bt45"/>
			<valueType key="newest10Meastimemax" title="max" buildTypeId="bt45"/>
			<valueType key="newest10Meastimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Retrieving newest 1000 measurements">
			<valueType key="newest1000Meastimemin" title="min" buildTypeId="bt45"/>
			<valueType key="newest1000Meastimemax" title="max" buildTypeId="bt45"/>
			<valueType key="newest1000Meastimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
        <graph title="Retrieving all measurements (up to 10000 limit)">
			<valueType key="allMeastimemin" title="min" buildTypeId="bt45"/>
			<valueType key="allMeastimemax" title="max" buildTypeId="bt45"/>
			<valueType key="allMeastimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Publishing 10 measurements through proxy (milliseconds)">
			<valueType key="false10publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="false10publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="false10publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Publishing 10 measurements direct (milliseconds)">
			<valueType key="true10publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="true10publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="true10publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Average Measurement Publishing Time (milliseconds)">
			<valueType key="false1000publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="false1000publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="false1000publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Benchmark Endpoint disable to COMMS_DOWN Time (milliseconds)">
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Benchmark Endpoint enable to COMMS_UP Time (milliseconds)">
			<valueType key="benchmarkCOMMSUPStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSUPStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSUPStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
	</custom-graphs>
</settings>