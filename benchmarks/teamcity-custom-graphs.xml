<settings>
	<custom-graphs>
		<graph title="Publishing 10 measurements through proxy (milliseconds)">
			<valueType key="false10publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="false10publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="false10publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Publishing 10 measurements direct (milliseconds)">
			<valueType key="true10publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="true10publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="true10publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Average Measurement Publishing Time (milliseconds)">
			<valueType key="false1000publishingTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="false1000publishingTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="false1000publishingTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Benchmark Endpoint disable to COMMS_DOWN Time (milliseconds)">
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSDOWNStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
		<graph title="Benchmark Endpoint enable to COMMS_UP Time (milliseconds)">
			<valueType key="benchmarkCOMMSUPStateTransmissionTimemin" title="min" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSUPStateTransmissionTimemax" title="max" buildTypeId="bt45"/>
			<valueType key="benchmarkCOMMSUPStateTransmissionTimeaverage" title="average" buildTypeId="bt45"/>
		</graph>
	</custom-graphs>
</settings>